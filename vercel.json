{
  "crons": [
    {
      "path": "/api/collect",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/pipeline/run",
      "schedule": "0 */3 * * *"
    },
    {
      "path": "/api/learn",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "app/api/pipeline/run/route.ts": {
      "maxDuration": 60
    },
    "app/api/collect/route.ts": {
      "maxDuration": 30
    },
    "app/api/learn/route.ts": {
      "maxDuration": 30
    }
  }
}